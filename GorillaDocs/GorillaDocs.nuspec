<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>GorillaDocs</id>
    <version>1.0.55.0</version>
    <title>GorillaDocs</title>
    <authors>Matthew Fitzmaurice</authors>
    <projectUrl>https://github.com/matthewfitzmaurice/GorillaDocs</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/matthewfitzmaurice/GorillaDocs/master/GorillaDocs/Resources/GorillaDocs.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      GorillaDocs is a collection of assemblies that makes working with Microsoft VSTO easier.

      This package contains the code that applies to all Microsoft Office applications.

      Specifically:
      - Extension methods
      - WPF helpers
      - Logging classes
    </description>
    <tags>VSTO Microsoft Office</tags>
    <dependencies>
      <dependency id="PostSharp" version="3.1.49" />
      <dependency id="log4net" version="2.0.3" />
      <dependency id="DataAnnotationsExtensions" version="1.1.0.0" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="PresentationCore" targetFramework=".NETFramework3.5, .NETFramework4.0" />
      <frameworkAssembly assemblyName="PresentationFramework" targetFramework=".NETFramework3.5, .NETFramework4.0" />
      <frameworkAssembly assemblyName="WindowsBase" targetFramework=".NETFramework3.5, .NETFramework4.0" />
      <frameworkAssembly assemblyName="System" targetFramework=".NETFramework3.5, .NETFramework4.0" />
      <frameworkAssembly assemblyName="System.Xaml" targetFramework=".NETFramework4.0" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="app.config.transform" target="content\app.config.transform" />
    <file src="bin\Release\zh-CHS\GorillaDocs.resources.dll" target="lib\net40\zh-CHS\GorillaDocs.resources.dll" />
    <file src="bin\Release\zh-CHT\GorillaDocs.resources.dll" target="lib\net40\zh-CHT\GorillaDocs.resources.dll" />
    <file src="bin\Release\GorillaDocs.dll" target="lib\net40\GorillaDocs.dll" />
  </files>
</package>